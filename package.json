  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-mq/MERNTutorial2022/issues"
  },
  "homepage": "https://github.com/j-mq/MERNTutorial2022#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.3",
    "typescript": "^4.8.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
